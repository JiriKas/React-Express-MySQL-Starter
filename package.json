{
  "name": "celinor_admin",
  "version": "1.0.0",
  "description": "Express Hello World on Render",
  "author": "Jirik",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm run frontend\"",
    "frontend": "npm run dev --prefix frontend",
    "backend": "npm start --prefix backend",
    "build:frontend": "npm run build --prefix frontend",
    "migrate": "npm run migrate --prefix backend",
    "install": "npm install --prefix backend && npm install --prefix frontend"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
